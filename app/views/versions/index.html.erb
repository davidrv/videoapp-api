<h1><%= @list.name %> history</h1>

<table class='table table-striped align-middle'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Changed at</th>
    </tr>
  </thead>

  <tbody>
    <% @versions.each do |version| %>
      <tr>
        <td><%= version.name %></td>
        <td><%= version.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br />
<%= link_to 'Back', list_items_path(@list), class: "btn btn-secondary" %>